{
  "name": "vectorial-database-rag",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec 'node --max-old-space-size=2048 --expose-gc' src/server.js",
    "start": "node --max-old-space-size=2048 --expose-gc src/server.js",
    "create-admin": "node src/infrastructure/scripts/create-admin.js",
    "docker:up": "docker start qdrant-rag redis-rag 2>/dev/null || (docker run -d --name qdrant-rag -p 6333:6333 -v \"$(pwd)/qdrant_data:/qdrant/storage\" qdrant/qdrant && docker run -d --name redis-rag -p 6379:6379 redis:7-alpine)",
    "docker:down": "docker stop qdrant-rag redis-rag",
    "docker:network:create": "docker network create vector-rag-network 2>/dev/null || echo 'Red vector-rag-network ya existe'",
    "docker:compose:up": "npm run docker:network:create && docker-compose up -d",
    "docker:compose:down": "docker-compose down"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [],
  "author": "Luis Leonidas Fern√°ndez",
  "license": "MIT",
  "dependencies": {
    "@qdrant/js-client-rest": "^1.16.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.5.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.2.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.20.0",
    "multer": "^2.0.2",
    "openai": "^6.9.1",
    "pdf-parse": "^2.4.5",
    "piscina": "^4.0.0",
    "resend": "^4.0.0",
    "undici": "^7.16.0",
    "uuid": "^13.0.0",
    "validator": "^13.13.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.11"
  }
}
